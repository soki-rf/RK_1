#include <iostream>
#include <cmath>

using namespace std;

int main()
{
    int n, i;
    double sm = 0;
    for(i=0; i < 10; ++i){
        n = i + 1;
        sm += 1/(pow(n, 3) + pow(n, 1/5));
        cout << n << ") " << 1/(pow(n, 3) + pow(n, 1/5)) << endl;
    }
    cout << "Sum = " << sm;
    return 0;
}
